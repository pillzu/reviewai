import { SvelteComponentTyped } from "svelte";
import type { Theme } from '../../theme';
declare const __propDef: {
    props: {
        /**
             * The theme object
             */ theme?: Theme | undefined;
        styles?: {};
    };
    events: {
        [evt: string]: CustomEvent<any>;
    };
    slots: {
        default: {};
    };
};
export type ChakraProviderProps = typeof __propDef.props;
export type ChakraProviderEvents = typeof __propDef.events;
export type ChakraProviderSlots = typeof __propDef.slots;
export default class ChakraProvider extends SvelteComponentTyped<ChakraProviderProps, ChakraProviderEvents, ChakraProviderSlots> {
}
export {};
